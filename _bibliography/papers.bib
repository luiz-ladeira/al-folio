	urldate = {2023-10-21},
	journal = {Revista ELO – Diálogos em Extensão},
	author = {Marriel, Nadja Biondine and Ladeira, Luiz Carlos Maia and Araújo, Renan Dos Santos and Silva, Janaina Da and Martins, Ana Luiza Pereira and Tavares, Mara Garcia},
	month = jun,
